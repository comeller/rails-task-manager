<h1>My tasks</h1>
<% @tasks.each do |task| %>
  <h3><%= link_to task.title, task_path(task)%></h3>
  <p><%= task.details %></p>
  <p><%= task.completed %></p>
  <%= link_to "Delete", delete_task_path(task),
        method: :delete,
        data: { confirm: "Are you sure?" } %>
<% end %>
  <p><%=link_to "Add new task", tasks_new_path %></p>
